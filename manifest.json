{
    "name": "Re-Inspector",
    "version": "0.0.1",
    "description": "Re-frame inspector",
    "devtools_page": "devtools.html",
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "web_accessible_resources" : ["/js/contentScript.js"],
    "externally_connectable": {
    "matches": ["http://localhost:3449/*"]
},
    "permissions": [
        "tabs",
        "http://*/*",
        "https://*/*"
    ],
    "manifest_version": 2
}
